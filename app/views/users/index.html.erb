<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Auth Token</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <% user = OpenStruct.new(user) %>
      <tr>
        <td><%= user.user["email"] %></td>
        <td><%= user.user["auth_token"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
